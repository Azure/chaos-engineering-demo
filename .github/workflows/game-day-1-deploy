name: Game Day 1 Deploy

on:
  workflow_dispatch:
  
jobs:
  call-build-deploy:
    strategy:
      matrix:
        team: [${{ env.GAME_DAY_TEAMS }}]
    uses: ./.github/workflows/build-deploy.yml
    with:
      team: ${{ matrix.team }}
    env:
      DEPLOYMENT_NAME: ${{ vars.DEPLOYMENT_NAME }}${{ matrix.team }}